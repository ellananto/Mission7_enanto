@{
    ViewData["Title"] = "Rental Form Submitted";
}

@model Mission6.Models.MovieFormResponse

@if (Model.Category is null | Model.Year is null | Model.Title is null | Model.Rating is null | Model.Director is null)
{
    <p>Please go back- looks like you missed a field!</p>
}
else
{
    <p>Thanks for your rental form submission.</p>
}